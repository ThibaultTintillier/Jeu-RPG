{
  "parserOptions": {
    "ecmaVersion": 2022
  },
  "env": {
      "browser": true,
      "es6": true
    },
  "extends": "eslint:recommended",
  "rules": {
    "semi": ["error", "always"],
    "quotes": ["error", "double"],
    "indent": ["error", 4],
    "no-var": "error",
    "camelcase": "error"
    }
  }